####################################################################################################
#  Build entries
####################################################################################################

all: class jar permissions

class:
	javac *.java

jar:
	jar -cfvm MipsHelper369.jar Manifest.mf *.class

permissions:
	chmod 700 MipsHelper369.jar

clean:
	rm -f *.class *.jar

